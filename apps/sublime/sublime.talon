app: Sublime Text
--

reveal: key(ctrl-shift-r)
pop: key(alt-[)
push: key(alt-])
enhance: key(ctrl-b)

find <user.sublime_find_type>: user.do_find(sublime_find_type)

special <user.bumble>: user.do_find(bumble)

go named: key(cmd-p)
zoom in: key(cmd-shift-+)
zoom out: key(cmd-shift--)
tab close: key(cmd-w)
make it so: key(cmd-s)

down: key(down arrow)
go up: key(up arrow)
# cmd: core.repeat_phrase(1)